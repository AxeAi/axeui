{
  "name": "axekit",
  "version": "0.1.1",
  "description": "A modern React component library built with Radix UI and Tailwind CSS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "tsup",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "component-library",
    "ui",
    "radix-ui",
    "tailwind",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/axeui"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-a11y": "^10.0.4",
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/addon-vitest": "^10.0.4",
    "@storybook/nextjs-vite": "^10.0.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser-playwright": "^4.0.7",
    "@vitest/coverage-v8": "^4.0.7",
    "class-variance-authority": "^0.7.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-storybook": "^10.0.4",
    "next": "16.0.1",
    "playwright": "^1.56.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storybook": "^10.0.4",
    "tailwindcss": "^4",
    "tsup": "^8.5.0",
    "typescript": "^5",
    "vitest": "^4.0.7"
  }
}
